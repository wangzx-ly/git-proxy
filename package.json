{
  "name": "git-proxy",
  "version": "1.0.0",
  "description": "代理某个 git repository 目录，使其可以通过http的方式来远程操作",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --fix",
    "lint:create": "eslint --init",
    "dev": "npx ts-node src/index.ts",
    "clean": "rm -rf dist/ && rm -f release/*",
    "webpack": "npx webpack",
    "release": "npm run clean && npm run webpack && cp dist/git-proxy.js dist/git-proxy.js.gz release/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/hgdharold/git-proxy.git"
  },
  "dependencies": {
    "koa": "^2.13.0",
    "koa-body": "^4.2.0",
    "koa-router": "^10.0.0",
    "simple-git": "^2.22.0"
  },
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@types/koa": "^2.11.6",
    "@types/koa-router": "^7.4.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "compression-webpack-plugin": "^6.1.1",
    "eslint": "^7.13.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  }
}
